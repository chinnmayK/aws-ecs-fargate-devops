{
  "family": "devtrack-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::202533520289:role/devtrack-task-exec-role",
  "containerDefinitions": [
    {
      "name": "devtrack-container",
      "image": "IMAGE_PLACEHOLDER",
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/devtrack",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "devtrack"
        }
      }
    }
  ]
}
